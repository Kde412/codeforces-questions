#include <bits/stdc++.h>
using namespace std;

int main()
{
    int t, n, m;
    std::cin >> t;
    while (t--)
    {
        cin >> n >> m;
        if (n == 2 && m == 3)
            cout<< "1 2"<<endl;
        else if(n == 3 && m == 2)
            cout<<"2 2"<<endl;
        else if(n == 3 && m == 3)
            cout<<"2 2"<<endl;
        else cout<<"1 1"<<endl;
    }

    return 0;
}
